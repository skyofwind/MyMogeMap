<?xml version="1.0" encoding="utf-8"?>
<!-- 用于设置信息对话框的圆角 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:angle="270"
                android:centerColor="#88ffffff"
                android:centerX="0.5"
                android:centerY="0.5"
                android:endColor="#ddffffff"
                android:startColor="#66ffffff"
                android:type="linear" />
        </shape>
    </item>
</layer-list>